{
    "root": true,
    "extends": "standard",
    "env": {
      "browser": true,
      "jquery": true
    },
    "rules": {
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "never",
            "asyncArrow": "always"
        }],
        "no-multiple-empty-lines": ["error", {
            "max": 2,
            "maxEOF": 0,
            "maxBOF": 0
        }]
    }
}
